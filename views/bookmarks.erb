<h1>List of Bookmarks</h1>
<ul>
  <%@bookmark.each do |bookmark|%>
    <li>
      <a href="<%=bookmark.url %>">
        <%=bookmark.title %>
      </a>
    </li>
  <% end %>
</ul>

<form action='/bookmark-saver' method='post'>
    Website: <input type='text' name='website'/>
    Bookmark Name: <input type='text' name='bookmark_name'/>
    <input type='submit' value='Add Bookmark'/>
</form>

<form action='/bookmark-deleter' method='post'>
    Delete Bookmark: <input type='text' name='bookmark_for_delete'/>
    <input type='submit' value='Confirm'/>
</form>
